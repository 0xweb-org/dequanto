{
  "name": "dequanto",
  "version": "0.0.4",
  "scripts": {
    "hardhat": "hardhat node",
    "build": "tsc -p tsconfig-src.json",
    "test": "atma --openssl-legacy-provider test --config='settings.base=./'"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.7.0",
    "hardhat": "^2.6.7",
    "shellbee": "^0.5.16",
    "ts-nameof": "^5.0.0"
  },
  "dependencies": {
    "@gnosis.pm/safe-core-sdk": "^2.1.0",
    "@gnosis.pm/safe-ethers-lib": "^1.1.0",
    "@gnosis.pm/safe-service-client": "^1.1.2",
    "@gnosis.pm/safe-web3-lib": "^1.1.0",
    "a-di": "^1.0.25",
    "alot": "^0.3.0",
    "appcfg": "^0.4.64",
    "atma": "^0.15.17",
    "atma-io": "^1.2.61",
    "atma-loader-ts": "^1.1.18",
    "atma-utils": "^0.2.51",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "ethers": "^5.5.1",
    "everlog": "^1.0.10",
    "includejs": "^0.16.5",
    "memd": "^0.3.11",
    "papaparse": "^5.3.1",
    "web3": "^1.6.0",
    "web3-core": "^1.6.0",
    "web3-core-subscriptions": "^1.6.0"
  },
  "atma": {
    "configs": "./configs/**.yml",
    "plugins": [
      "atma-loader-ts"
    ],
    "settings": {
      "include": {
        "amd": true,
        "extentionDefault": {
          "js": "ts"
        },
        "routes": "#import ./tsconfig-build.json compilerOptions.paths",
        "map": {}
      },
      "atma-loader-ts": {
        "sourceMap": true,
        "typescript": "#import ./tsconfig-build.json"
      }
    }
  }
}
